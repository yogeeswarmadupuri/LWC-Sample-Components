<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_Greater_Than_End_date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT  ISBLANK( End_Date_Time__c ), End_Date_Time__c &lt;= ( Start_DateTime__c +1) )</errorConditionFormula>
    <errorDisplayField>End_Date_Time__c</errorDisplayField>
    <errorMessage>The End date should be Greater Than Start Date</errorMessage>
</ValidationRule>
